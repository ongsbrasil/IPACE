<?xml version="1.0"?>
<?mso-application progid="Excel.Sheet"?>
<Workbook xmlns="urn:schemas-microsoft-com:office:spreadsheet"
 xmlns:o="urn:schemas-microsoft-com:office:office"
 xmlns:x="urn:schemas-microsoft-com:office:excel"
 xmlns:ss="urn:schemas-microsoft-com:office:spreadsheet"
 xmlns:html="http://www.w3.org/TR/REC-html40">
 <Styles>
  <Style ss:ID="Default" ss:Name="Normal">
   <Alignment ss:Vertical="Bottom"/>
   <Borders/>
   <Font ss:FontName="Arial" x:Family="Swiss" ss:Size="10" ss:Color="#000000"/>
   <Interior/>
   <NumberFormat/>
   <Protection/>
  </Style>
  <Style ss:ID="sHeader">
   <Font ss:FontName="Arial" x:Family="Swiss" ss:Size="12" ss:Color="#FFFFFF" ss:Bold="1"/>
   <Interior ss:Color="#000080" ss:Pattern="Solid"/>
  </Style>
  <Style ss:ID="sBold">
   <Font ss:FontName="Arial" x:Family="Swiss" ss:Size="10" ss:Bold="1"/>
  </Style>
 </Styles>
 <Worksheet ss:Name="Instruções">
  <Table>
   <Column ss:Width="150"/>
   <Column ss:Width="400"/>
   <Row>
    <Cell ss:StyleID="sHeader"><Data ss:Type="String">Campo</Data></Cell>
    <Cell ss:StyleID="sHeader"><Data ss:Type="String">Regra</Data></Cell>
   </Row>
   <Row>
    <Cell ss:StyleID="sBold"><Data ss:Type="String">Nome</Data></Cell>
    <Cell><Data ss:Type="String">Nome completo do aluno</Data></Cell>
   </Row>
   <Row>
    <Cell ss:StyleID="sBold"><Data ss:Type="String">Data Nascimento</Data></Cell>
    <Cell><Data ss:Type="String">Formato AAAA-MM-DD (Ex: 2010-05-15)</Data></Cell>
   </Row>
   <Row>
    <Cell ss:StyleID="sBold"><Data ss:Type="String">RG</Data></Cell>
    <Cell><Data ss:Type="String">Formato XX.XXX.XXX-X</Data></Cell>
   </Row>
   <Row>
    <Cell ss:StyleID="sBold"><Data ss:Type="String">Sexo</Data></Cell>
    <Cell><Data ss:Type="String">"Masculino", "Feminino" ou "Outro"</Data></Cell>
   </Row>
   <Row>
    <Cell ss:StyleID="sBold"><Data ss:Type="String">Modalidade</Data></Cell>
    <Cell><Data ss:Type="String">Deve ser idêntico à tabela na aba "Horários"</Data></Cell>
   </Row>
   <Row>
    <Cell ss:StyleID="sBold"><Data ss:Type="String">Horário</Data></Cell>
    <Cell><Data ss:Type="String">Deve ser idêntico à tabela na aba "Horários"</Data></Cell>
   </Row>
   <Row>
    <Cell ss:StyleID="sBold"><Data ss:Type="String">Data Entrada</Data></Cell>
    <Cell><Data ss:Type="String">Opcional. Formato AAAA-MM-DD. Se vazio, usa data atual.</Data></Cell>
   </Row>
   <Row>
    <Cell ss:StyleID="sBold"><Data ss:Type="String">G</Data></Cell>
    <Cell><Data ss:Type="String">Opcional. "Sim" ou "Não". (Grávida?)</Data></Cell>
   </Row>
   <Row>
    <Cell ss:StyleID="sBold"><Data ss:Type="String">PCD</Data></Cell>
    <Cell><Data ss:Type="String">Opcional. "Sim" ou "Não". (Pessoa com Deficiência)</Data></Cell>
   </Row>
  </Table>
 </Worksheet>
